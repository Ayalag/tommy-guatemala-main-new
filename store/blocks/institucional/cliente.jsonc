{
    "store.custom#cliente": {
        "blocks": [
            "responsive-layout.desktop#desktop-cliente",
            "responsive-layout.mobile#mobile-cliente"
        ]
    },
    "responsive-layout.desktop#desktop-cliente": {
        "children": [
            "flex-layout.row#contenedor-cliente"
        ]
    },
    "flex-layout.row#contenedor-cliente": {
        "children": [
            "flex-layout.row#contenedor-cliente-general"
        ],
        "props": {
            "blockClass": "contenedor-institucional"
        }
    },
    "flex-layout.row#contenedor-cliente-general": {
        "children": [
            "flex-layout.col#menu-cliente",
            "flex-layout.col#contenedor-cliente"
        ],
        "props": {
            "blockClass": "contenedor-general-institucional",
            "preventVerticalStretch": true,
            "preventHorizontalStretch": true
        }
    },
    "flex-layout.col#menu-cliente": {
        "children": [
            "rich-text#menu-envio-cliente",
            "rich-text#menu-devolucion-cliente",
            "rich-text#menu-localizador-cliente",
            "rich-text#menu-tallas-cliente",
            "rich-text#menu-cliente-cliente",
            "rich-text#menu-nosotros-cliente",
            "rich-text#menu-prensa-cliente",
            "rich-text#menu-terminos-cliente",
            "rich-text#menu-pp-cliente",
            "rich-text#menu-pc-cliente"
        ],
        "props": {
            "blockClass": "menu-institucional",
            "preventVerticalStretch": true
        }
    },
    "rich-text#menu-cliente-cliente": {
        "props": {
            "text": "[Servicio al Cliente](/servicio-cliente)",
            "textAlignment": "LEFT",
            "textPosition": "LEFT",
            "blockClass": "menu-institucional-tab-active"
        }
    },
    "rich-text#menu-localizador-cliente": {
        "props": {
            "text": "[Localizador de Tiendas](/localizador-tiendas)",
            "textAlignment": "LEFT",
            "textPosition": "LEFT",
            "blockClass": "menu-institucional-tab-active"
        }
    },
    "rich-text#menu-tallas-cliente": {
        "props": {
            "text": "[Guía de Tallas](/guia-tallas)",
            "textAlignment": "LEFT",
            "textPosition": "LEFT",
            "blockClass": "menu-institucional-tab-active"
        }
    },
    "rich-text#menu-devolucion-cliente": {
        "props": {
            "text": "[Devoluciónes](/terminos-devoluciones)",
            "textAlignment": "LEFT",
            "textPosition": "LEFT",
            "blockClass": "menu-institucional-tab-active"
        }
    },
    "rich-text#menu-envio-cliente": {
        "props": {
            "text": "[Envios](/terminos-envio)",
            "textAlignment": "LEFT",
            "textPosition": "LEFT",
            "blockClass": "menu-institucional-tab-active"
        }
    },
    "rich-text#menu-nosotros-cliente": {
        "props": {
            "text": "[Sobre Nosotros](https://global.tommy.com/en_int/about-us)",
            "textAlignment": "LEFT",
            "textPosition": "LEFT",
            "blockClass": "menu-institucional-tab-active"
        }
    },
    "rich-text#menu-prensa-cliente": {
        "props": {
            "text": "[Sala de Prensa](https://global.tommy.com/en_int/newsroom)",
            "textAlignment": "LEFT",
            "textPosition": "LEFT",
            "blockClass": "menu-institucional-tab-active"
        }
